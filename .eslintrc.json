{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:jsx-a11y/recommended"
    ],  
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "jsx-a11y/rule-name": 2
    },
    "plugins": ["jsx-a11y"]
}
